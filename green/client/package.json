{
  "name": "angularjs-template",
  "version": "0.0.4",
  "description": "A starter project for AngularJS based on John Papa's style guide",
  "keywords": [
    "AngularJS",
    "angular",
    "template",
    "angularjs-template",
    "seed",
    "johnpapa",
    "styleguide",
    "node",
    "express"
  ],
