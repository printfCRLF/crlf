    "angular-material": "^1.1.10",
    "angular-messages": "^1.7.2",
    "angular-ui-calendar": "^1.0.2",
    "angular-ui-router": "^1.0.18",
    "fullcalendar": "^3.9.0",
    "underscore": "^1.9.1"
